<% title = "The Code Center | Registration Page" %>
<%- include('partials/header') %>

<script>
    
</script>

<h3>Registered Students</h3>

   
<table border="1">

    <tr>
        <th>Name</th><th>Gender</th><th>Course</th><th>Registration Date</th>
        <th>Edit</th><th>Delete</th>
            
        </th>
    </tr>
    <tr>
        <% record.forEach(function(records){%>
            <td><%= records.firstName + ' '+records.lastName %></td>
            <td><%= records.gender %></td>
            <td><%= records.course %></td>
            <td><%= records.dateRegistered %></td>
            <td><a href="/edit/<%= records._id %>">Edit</a></td>
            <td><a href="/delete/<%= records._id %>" onclick="return confirm('Are you sure you want to delete this record?');"
               
                   
                
                >Delete</a></td>
    </tr>
<% }) %>
</table>